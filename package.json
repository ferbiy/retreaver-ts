{
  "name": "@retreaver/retreaver-js",
  "version": "1.0.0",
  "description": "Retreaver JavaScript API - TypeScript edition",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:browser": "webpack --config webpack.config.js",
    "build:dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "ts-mocha test/**/*.test.ts",
    "test:watch": "ts-mocha test/**/*.test.ts --watch",
    "docs": "typedoc src/index.ts --out docs",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "retreaver",
    "callpixels",
    "phone",
    "tracking",
    "call",
    "number",
    "campaign",
    "typescript"
  ],
  "author": {
    "name": "Blake Hilscher",
    "email": "blake@retreaver.com",
    "url": "http://retreaver.com/"
  },
  "contributors": [
    {
      "name": "Blake Hilscher",
      "email": "blake@hilscher.ca",
      "url": "http://blake.hilscher.ca/"
    },
    {
      "name": "Jason Kay",
      "email": "jason@retreaver.com",
      "url": "http://retreaver.com/"
    },
    {
      "name": "Ahmed El-Daly",
      "email": "aeldaly@gmail.com",
      "url": "http://retreaver.com/"
    }
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/retreaver/retreaverjs.git"
  },
  "bugs": {
    "url": "https://github.com/retreaver/retreaverjs/issues"
  },
  "homepage": "https://github.com/retreaver/retreaverjs",
  "engines": {
    "node": ">= 14.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "ts-loader": "^9.4.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/chai": "^4.3.0",
    "ts-mocha": "^10.0.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "typedoc": "^0.25.0",
    "marked": "^4.3.0",
    "terser-webpack-plugin": "^5.3.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}